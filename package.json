{
    "$schema": "https://www.raycast.com/schemas/extension.json",
    "name": "psyho",
    "title": "local extensions",
    "description": "Hello raycast API!",
    "icon": "command-icon.png",
    "author": "Psyho",
    "license": "MIT",
    "commands": [
        {
            "name": "list",
            "title": "Show List and Detail",
            "description": "Template for showing a static list and a detail view",
            "mode": "view"
        },
        {
            "name": "doppler",
            "icon": "doppler-icon.png",
            "title": "doppler",
            "description": "Share secrets",
            "mode": "view"
        },
        {
            "name": "todo",
            "icon": "todo-icon.png",
            "title": "todo list",
            "description": "Simple TODO list",
            "mode": "view"
        },
        {
            "name": "startTimer",
            "icon": "hourglass-icon-white.png",
            "title": "start timer",
            "description": "Set Horo timer",
            "mode": "view"
        },
        {
            "name": "deleteTimer",
            "icon": "hourglass-icon-white.png",
            "title": "delete timer",
            "description": "Delete Horo timer",
            "mode": "no-view"
        },
        {
            "name": "urlShortener",
            "icon": "shortener-icon.png",
            "title": "url shortener",
            "description": "Shorten your URL",
            "mode": "view"
        }
    ],
    "dependencies": {
        "@raycast/api": "^1.38.2",
        "got": "^12.3.0",
        "nanoid": "^4.0.0",
        "run-applescript": "^6.0.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.31.0",
        "@typescript-eslint/parser": "^5.31.0",
        "eslint": "^8.20.0",
        "eslint-config-prettier": "^8.3.0",
        "prettier": "^2.5.1",
        "typescript": "^4.4.3"
    },
    "scripts": {
        "build": "ray build -e dist",
        "dev": "ray develop",
        "fix-lint": "ray lint --fix",
        "lint": "ray lint"
    }
}
